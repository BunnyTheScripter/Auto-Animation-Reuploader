local ScriptEditorService = game:GetService("ScriptEditorService")
local HttpService = game:GetService("HttpService")
local MarketPlaceService = game:GetService("MarketplaceService")
local SoundService = game:GetService("SoundService")
local Signal = require(script.Signal)

local toolbar = plugin:CreateToolbar("kart's tools")
local pluginButton = toolbar:CreateButton("kart's tools", "Launch the python script first", "http://www.roblox.com/asset/?id=18239419915")
local notificationUi = script.NotificationUi
local gui = script.Frame
local notificationId = 0
local started = false
local busy = false

local widget = plugin:CreateDockWidgetPluginGui(
	"spoofer",
	DockWidgetPluginGuiInfo.new(
		Enum.InitialDockState.Float,
		false,
		false,
		300,
		200,
		300,
		200
	)
)

widget.Title = "sub to kartfr"
script.NotificationUi.Parent = game:GetService("CoreGui")
gui.Parent = widget

local function getNotificationId()
	notificationId += 1
	return notificationId
end

local function playSound(sound: Sound)
	local newSound = sound:Clone()
	newSound.Parent = widget
	newSound:Play()
	newSound.Ended:Wait()
	newSound:Destroy()
end

local function sendNotification(title: string, message: string, titleColor: Color3?)
	local notification = script.Notification:Clone()
	notification.Title.Text = title
	notification.Description.Text = message
	notification.Title.TextColor3 = titleColor or Color3.new(1, 0, 0)
	notification.LayoutOrder = getNotificationId()
	
	if titleColor == Color3.new(0, 1, 0) then
		task.spawn(playSound, script.Success)
	else
		local random = Random.new(os.clock() * 10000)
		if random:NextInteger(1, 10) == 10 then
			task.spawn(playSound, script.Boom) -- funny XD easter eg hah
		else
			task.spawn(playSound, script.Error)
		end
	end
	
	notification.Parent = notificationUi.Frame
	task.delay(2, notification.Destroy, notification)
end

local function try(...)
	local success, result
	for i=1, 3 do
		if success then break end
		if i > 1 then task.wait(1) end
		success, result = pcall(...)
	end
	return success, result
end

local function testForPermissions()
	local testScript = Instance.new("Script")
	local success = pcall(ScriptEditorService.UpdateSourceAsync, ScriptEditorService, testScript, function() return "" end)
	testScript:Destroy()
	if success then return true end
end

local function isScript(instance)
	return instance:IsA("Script") or instance:IsA("ModuleScript") or instance:IsA("LocalScript")
end

local function isValue(instance)
	return instance:IsA("NumberValue") or instance:IsA("StringValue")
end

local function isValidId(id: string)
	return #id > 7 and #id < 13 and string.find(id, "[^0]")
end

local function getInstanceIds(mainType: "Sound" | "Animation", instances: { any })
	local idsToQuery = {}
	local loopedOverAnimations = {}
	local mainInstances = {}
	local skippedMainInstances = {}
	local scriptIdInfo = {}
	local valueInstances = {}

	for i,v in instances do
		local isAScript = isScript(v)
		local isAValueInstance = isValue(v)
		local isAMainType = v:IsA(mainType)
		if not isAMainType and not isAScript and not isAValueInstance then continue end
		

		if isAScript then
			local scriptSource = v.Source
			local lastEnd = 0

			while true do
				local idStart, idEnd = scriptSource:find("%d+", lastEnd)
				if not idStart then break end
				local id = scriptSource:sub(idStart, idEnd)
				
				lastEnd = idEnd + 1

				if not isValidId(id) then continue end
				if scriptIdInfo[id] then table.insert(scriptIdInfo[id], { idStart, idEnd, v }) continue end

				scriptIdInfo[id] = {}
				table.insert(scriptIdInfo[id], { idStart, idEnd, v })
				
				if loopedOverAnimations[id] then continue end
				table.insert(idsToQuery, id) 
				loopedOverAnimations[id] = true
			end

			continue
		end
		

		local id = string.gsub(isAValueInstance and v.Value or v.AnimationId, "%D", "")
		if not isValidId(id) then 
			if isAMainType then table.insert(skippedMainInstances, v) end
			continue
		end
		
		local targetArray = isAMainType and mainInstances or valueInstances
		if targetArray[id] then 
			table.insert(targetArray[id], v)
			continue 
		end

		(isAMainType and mainInstances or valueInstances)[id] = {}
		table.insert(isAMainType and mainInstances[id] or valueInstances[id], v)

		if not loopedOverAnimations[id] then 
			table.insert(idsToQuery, id) 
			loopedOverAnimations[id] = true
		end
	end
	
	return idsToQuery, skippedMainInstances, mainInstances, scriptIdInfo, valueInstances
end

local function getIdsFromLocalHost(assetType, mainInstances, scriptIdInfo, valueInstances)
	local scriptLengthChange = {}
	local idIndex = assetType == "Animation" and "AnimationId" or "SoundId"
	
	while task.wait(1) do
		local success, idsToChange = pcall(HttpService.GetAsync, HttpService, `http://localhost:{ANIMATIONLOCALHOST}`)
		if not success then sendNotification("Stopped Uploading", "Disconnected from localhost.") warn("Disconnected from localhost.") break end
		if idsToChange == "done" then sendNotification("Notification", "Finished reuploading all animations.", Color3.new(0, 1, 0)) warn("Finished reuploading all animations.") break end
		if idsToChange == "" then continue end

		idsToChange = HttpService:JSONDecode(idsToChange)

		for oldId, newId in idsToChange do
			if mainInstances[oldId] then
				for i, instance in mainInstances[oldId] do
					instance[idIndex] = "rbxassetid://" .. newId
				end

				warn(`Changed all animation ids: {oldId} ; {newId}`, "Instances:", table.unpack(mainInstances[oldId]))
			end

			if valueInstances[oldId] then
				for i, instance in valueInstances[oldId] do
					if instance:IsA("NumberValue") then instance.Value = newId continue end 
					
					local oldValue = instance.Value
					local oldIdLength = #oldId
					local idStart, idEnd = string.find(oldValue, oldId)
					
					instance.Value = oldValue:sub(1, idStart - 1) .. newId .. oldValue:sub(idEnd + 1)
				end

				warn(`Changed all instance values: {oldId} ; {newId}`, "Instances:", table.unpack(valueInstances[oldId]))
			end

			if not scriptIdInfo[oldId] then continue end
			local scriptsChanged = {}

			for i, animationInfo in scriptIdInfo[oldId] do
				local idStart = animationInfo[1]
				local idEnd = animationInfo[2]
				local animationScript = animationInfo[3]

				table.insert(scriptsChanged, animationScript)

				if scriptLengthChange[animationScript] then
					for i,v in scriptLengthChange[animationScript]  do
						local index = v[1]
						local difference = v[2]

						if index < idStart then
							idStart += difference
						end

						if index < idEnd then
							idEnd += difference
						end
					end
				end
				
				task.spawn(ScriptEditorService.UpdateSourceAsync, ScriptEditorService, animationScript, function(oldSource) -- hopefully this breaks nothing :innocent:
					return oldSource:sub(1, idStart - 1) .. newId .. oldSource:sub(idEnd + 1)
				end)

				if not scriptLengthChange[animationScript] then scriptLengthChange[animationScript] = {} end
				local idDifference = #newId - #oldId
				if idDifference == 0 then continue end
				table.insert(scriptLengthChange[animationScript], {idStart, idDifference})
			end

			warn(`Changed all animation ids: {oldId} ; {newId}`, "Scripts:", table.unpack(scriptsChanged))
		end
	end
end

local function startUploading(assetType: "Sound" | "Animation", instances: { any })
	if not testForPermissions() then sendNotification("Error", "Plugin needs script injection permission.") return end
	if game.CreatorId == 0 then sendNotification("Error", "Publish game to group/profile.") return end
	if busy then sendNotification("Busy", "Plugin is busy.") return end
	local localHostStarted = pcall(HttpService.GetAsync, HttpService, `http://localhost:{ANIMATIONLOCALHOST}`)
	if not localHostStarted then sendNotification("Error", "Could not connect to local host.") return end

	busy = true
	sendNotification("Connected", "Successfully connected to localhost", Color3.new(0, 1, 0))
	
	local idsToQuery, skippedMainInstances, mainInstances, scriptIdInfo, valueInstances = getInstanceIds(assetType, instances)
	
	sendNotification("Notification", `Sending {#idsToQuery} ids to localhost`, Color3.new(0, 1, 0))
	warn(`Sending {#idsToQuery} ids to localhost. Skipped {assetType} instances:`, table.unpack(skippedMainInstances))

	local successfullySentAnimations, result = try(
		HttpService.PostAsync, 
		HttpService, 
		`http://localhost:{ANIMATIONLOCALHOST}`, 
		HttpService:JSONEncode({isGroup = game.CreatorType == Enum.CreatorType.Group, creatorId = game.CreatorId, ids = idsToQuery, uploadType = assetType})
	)

	if not successfullySentAnimations then 
		sendNotification("Stopped Uploading", "Failed to send ids to localhost.") 
		warn(`Failed to send ids to localhost.\nError: {result}`)
		busy = false
		return 
	end

	getIdsFromLocalHost(assetType, mainInstances, scriptIdInfo, valueInstances)

	busy = false
end

gui.ImageLabel.AnimationButton.MouseButton1Down:Connect(function()
	startUploading("Animation", game:GetDescendants())
end)

gui.ImageLabel.SoundButton.MouseButton1Down:Connect(function()
	sendNotification("Not Availabe", "Coming soon...")
end)

pluginButton.Click:Connect(function()
	widget.Enabled = not widget.Enabled
end)]]></ProtectedString>
